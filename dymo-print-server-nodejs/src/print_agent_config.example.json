{
  "backend_url": "https://your-backend-server.com",
  "api_token": "your-api-token-here",
  "api_endpoints": {
    "get_jobs": "/api/printing/api/jobs/",
    "update_status": "/api/printing/api/jobs/{job_id}/update_status/"
